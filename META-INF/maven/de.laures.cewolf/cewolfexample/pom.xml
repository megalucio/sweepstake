<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>de.laures.cewolf</groupId>
  <artifactId>cewolfexample</artifactId>
  <packaging>war</packaging>
  <version>1.0</version>
  <build>
    <sourceDirectory>e:\STC\Workspace\cewolfexample\src\main\java</sourceDirectory>
    <scriptSourceDirectory>src/main/scripts</scriptSourceDirectory>
    <testSourceDirectory>e:\STC\Workspace\cewolfexample\src\test\java</testSourceDirectory>
    <outputDirectory>e:\STC\Workspace\cewolfexample\target\classes</outputDirectory>
    <testOutputDirectory>e:\STC\Workspace\cewolfexample\target\test-classes</testOutputDirectory>
    <resources>
      <resource>
        <directory>e:\STC\Workspace\cewolfexample\src\main\resources</directory>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>e:\STC\Workspace\cewolfexample\src\test\resources</directory>
      </testResource>
    </testResources>
    <directory>e:\STC\Workspace\cewolfexample\target</directory>
    <finalName>cewolfexample</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <version>2.0</version>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.0</version>
      </plugin>
      <plugin>
        <artifactId>maven-install-plugin</artifactId>
        <version>2.0</version>
      </plugin>
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <version>2.1</version>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.0</version>
      </plugin>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>2.0-beta-2-Brian</version>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Maven Repository Switchboard</name>
      <url>http://repo1.maven.org/maven2</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <releases>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Maven Plugin Repository</name>
      <url>http://repo1.maven.org/maven2</url>
    </pluginRepository>
  </pluginRepositories>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>jfree</groupId>
      <artifactId>jfreechart-demo</artifactId>
      <version>1.0.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>cewolf</groupId>
      <artifactId>cewolf</artifactId>
      <version>1.0</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <reporting>
    <outputDirectory>target/site</outputDirectory>
  </reporting>
</project>